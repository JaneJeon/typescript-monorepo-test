{
  "targets": {
    "start": {
      "dependsOn": ["build"],
      "executor": "nx:run-script",
      "options": {
        "script": "start"
      }
    },
    "generate-openapi": {
      "dependsOn": ["build"],
      "executor": "nx:run-script",
      "options": {
        "script": "generate-openapi"
      },
      "outputs": ["{workspaceRoot}/openapi.yml"]
    }
  }
}
