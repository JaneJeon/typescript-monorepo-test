{
  "name": "backend",
  "private": true,
  "description": "This project was bootstrapped with Fastify-CLI.",
  "type": "module",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start:zod": "fastify start -l info dist/app-zod.js",
    "start:typebox": "fastify start -l info dist/app-typebox.js",
    "build": "concurrently 'npm:build:*'",
    "build:ts": "tsc",
    "watch": "concurrently 'npm:watch:*'",
    "watch:ts": "tsc -w",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.5.1",
    "@fastify/swagger-ui": "^1.8.1",
    "@fastify/type-provider-typebox": "^3.2.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "fastify-type-provider-zod": "^1.1.9",
    "shared": "*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.1.3"
  }
}
