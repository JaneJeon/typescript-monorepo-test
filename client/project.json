{
  "targets": {
    "generate-openapi": {
      "executor": "nx:run-script",
      "dependsOn": ["^generate-openapi"],
      "options": {
        "script": "generate-openapi"
      },
      "inputs": ["{workspaceRoot}/openapi.yml"],
      "outputs": ["{projectRoot}/src/**/*.ts"]
    },
    "build": {
      "executor": "nx:run-script",
      "dependsOn": ["generate-openapi"],
      "options": {
        "script": "build"
      }
    }
  }
}
